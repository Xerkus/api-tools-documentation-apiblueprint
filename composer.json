{
    "name": "laminas-api-tools/api-tools-documentation-apiblueprint",
    "description": "Laminas API Tools API Blueprint Documentation Module",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "module",
        "documentation",
        "API Blueprint",
        "apiary"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-documentation-apiblueprint/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-documentation-apiblueprint",
        "rss": "https://github.com/laminas-api-tools/api-tools-documentation-apiblueprint/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.2-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "laminas-api-tools/api-tools-api-problem": "~1.0",
        "laminas-api-tools/api-tools-content-negotiation": "~1.0",
        "laminas-api-tools/api-tools-documentation": "~1.0",
        "laminas/laminas-eventmanager": "^2.4.0",
        "laminas/laminas-mvc": "^2.4.0",
        "laminas/laminas-servicemanager": "^2.4.0",
        "laminas/laminas-view": "^2.4.0",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3.1"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\Documentation\\ApiBlueprint\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ApiTools\\Documentation\\ApiBlueprint\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-apigility-documentation-apiblueprint": "self.version"
    }
}
